{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/user/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/user/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/user",
  "projects": {
    "analyzing-agent": {
      "type": "application",
      "root": "apps/analyzing-agent",
      "entryFile": "main",
      "sourceRoot": "apps/analyzing-agent/src",
      "compilerOptions": {
        "tsConfigPath": "apps/analyzing-agent/tsconfig.app.json"
      }
    },
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "case": {
      "type": "application",
      "root": "apps/case",
      "entryFile": "main",
      "sourceRoot": "apps/case/src",
      "compilerOptions": {
        "tsConfigPath": "apps/case/tsconfig.app.json"
      }
    },
    "closing-agent": {
      "type": "application",
      "root": "apps/closing-agent",
      "entryFile": "main",
      "sourceRoot": "apps/closing-agent/src",
      "compilerOptions": {
        "tsConfigPath": "apps/closing-agent/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "intake-agent": {
      "type": "application",
      "root": "apps/intake-agent",
      "entryFile": "main",
      "sourceRoot": "apps/intake-agent/src",
      "compilerOptions": {
        "tsConfigPath": "apps/intake-agent/tsconfig.app.json"
      }
    },
    "processing-agent": {
      "type": "application",
      "root": "apps/processing-agent",
      "entryFile": "main",
      "sourceRoot": "apps/processing-agent/src",
      "compilerOptions": {
        "tsConfigPath": "apps/processing-agent/tsconfig.app.json"
      }
    },
    "scoping-agent": {
      "type": "application",
      "root": "apps/scoping-agent",
      "entryFile": "main",
      "sourceRoot": "apps/scoping-agent/src",
      "compilerOptions": {
        "tsConfigPath": "apps/scoping-agent/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    }
  }
}